{
  "name": "rmg-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "pretypeorm": "(rm ormconfig.json || :) && ts-node -r tsconfig-paths/register dist/helpers/write-type-orm-config.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "yarn run typeorm -- migration:generate -n",
    "typeorm:migration:run": "yarn run typeorm -- migration:run",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "run-script-os",
    "start:default": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "TZ=UTC jest src --runInBand",
    "test:watch": "TZ=UTC jest src --runInBand --watch",
    "test:cov": "TZ=UTC jest src --runInBand --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prepare": "husky install",
    "test:integration": "TZ=UTC jest test/integration --runInBand"
  },
  "dependencies": {
    "@babel/traverse": "^7.18.8",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^8.4.7",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^8.0.0",
    "@types/jest-when": "^3.5.2",
    "class-transformer": "^0.5.1",
    "eslint-plugin-jest": "^26.6.0",
    "nest-winston": "^1.7.0",
    "nestjs-typeorm-paginate": "^4.0.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typeorm": "^0.2.45",
    "webpack": "^5.73.0",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^8.4.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^18.0.4",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^27.3.0",
    "jest-when": "^3.5.1",
    "prettier": "^2.7.1",
    "run-script-os": "^1.1.6",
    "source-map-support": "^0.5.20",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2"
  }
}
